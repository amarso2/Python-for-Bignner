data=[-2,34,3,23,54,3,23,23]
print("Before sorting data is",data)
for i in range(len(data)):
    for j in range(0, len(data) - 1 - i):
        if data[j] > data[j + 1]:
            # # Without third value swap
            data[j], data[j + 1] = data[j + 1], data[j]
        # # With third value swap
         # temp=data[j]
         # data[j]=data[j+1]
         # data[j+1]=temp

print("After sorting data is ",data)
